    "def mul(i):\n",
    "\n",
    "    c = 1\n",
    "\n",
    "    for x in i:\n",
    "\n",
    "        c*= x\n",
    "\n",
    "    return c\n",
    "\n",
    "list=[4,8,2,3,5]\n",
    "\n",
    "print(mul(list))"
   ]
