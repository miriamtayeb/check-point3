    "def last(n): return n[-1]\n",
    "\n",
    "def sort_list_last(tuples):\n",
    "  return sorted(tuples, key=last)\n",
    "\n",
    "print(sort_list_last([(7, 5), (5, 3), (6, 4), (9, 1), (8, 1)]))"
   ]
